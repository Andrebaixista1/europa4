{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/neo-logo.svg" },
    { "src": "/favicon.svg", "dest": "/neo-logo.svg" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
