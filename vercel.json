{
  "version": 2,
  "functions": {
    "api/consulta-v8/*.js": { "maxDuration": 300 },
    "api/consulta-v8/**/*.js": { "maxDuration": 300 }
  },
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "routes": [
    { "src": "/api/n8n", "dest": "/api/n8n/[...path].js" },
    { "src": "/api/n8n/(.*)", "dest": "/api/n8n/[...path].js" },
    { "src": "/api/consulta-v8", "dest": "/api/consulta-v8/[...path].js" },
    { "src": "/api/consulta-v8/(.*)", "dest": "/api/consulta-v8/[...path].js?path=$1" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
